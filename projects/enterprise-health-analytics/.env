# Sahha API Configuration
SAHHA_API_URL=https://sandbox-api.sahha.ai
SAHHA_AUTH_TOKEN=your-sahha-auth-token-here

# Export Configuration
OUTPUT_DIR=./exports
DATE_FORMAT=YYYY-MM-DD

# Enterprise Configuration
ORGANIZATION_NAME=DemoOrganization
DEFAULT_EMPLOYEE_COUNT=25
DEFAULT_DAYS=30

# Trucking Company Specific
TRUCKING_COMPANY_NAME=DemoTruckingCorp
DEFAULT_DRIVER_COUNT=50

# Military Unit Specific
MILITARY_UNIT_NAME=DemoMilitaryPlatoon
DEFAULT_SOLDIER_COUNT=40

# Performance Settings
MAX_RECORDS_PER_FILE=50000
PARALLEL_PROCESSING=true
MEMORY_LIMIT=512MB